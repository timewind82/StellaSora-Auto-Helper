    "next": [
      "通用_判断是否在主页"
    ],
    "recognition": {
      "param": {
        "template": [
          "home.png",
          "chat_shutdown.png"
        ]
      },
      "type": "TemplateMatch"
    },
    "repeat": 2,
    "timeout": 1000
  }
