    "on_error": [
      "通用_返回主页"
    ],
    "recognition": {
      "param": {
        "template": [
          "menu.png"
        ]
      },
      "type": "TemplateMatch"
    },
    "timeout": 100
  },
  "通用_点击空白处继续": {
    "action": {
      "param": {},
      "type": "Click"
    },
    "recognition": {
      "param": {
        "expected": [
          "点击空白处继续"
        ]
      },
      "type": "OCR"
    }
  },
  "通用_返回主页": {
    "action": {
      "param": {},
      "type": "Click"
    },
    "anchor": [
      "返回主页"
    ],
    "focus": {
      "Node.Recognition.Starting": "正在返回主页",
      "Node.Recognition.Succeeded": ""
